<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/loginBackground"
              android:orientation="vertical">
    <RelativeLayout
        android:id="@+id/personal_setting_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.1"
        android:background="@mipmap/personal_background">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="19dp"
            android:layout_marginStart="19dp"
            android:layout_toEndOf="@+id/profile_image"
            android:layout_toRightOf="@+id/profile_image"
            android:textSize="20sp"
            tools:text="孤独患者"/>
        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="61dp"
            android:layout_marginRight="61dp"
            android:layout_marginTop="12dp"
            android:background="@color/transparency"
            android:src="@mipmap/personal_set_up"/>
        <ImageButton
            android:id="@+id/dddd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="23.5dp"
            android:layout_marginRight="23.5dp"
            android:layout_marginTop="12dp"
            android:background="@color/transparency"
            android:src="@mipmap/personal_message"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true" android:layout_centerVertical="true"
            android:layout_marginEnd="11dp" android:layout_marginRight="11dp"
            android:src="@mipmap/personal_information_enter"/>

    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="11dp"
        android:layout_weight="1"
        android:background="@color/loginBackground"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/myOrder_rlyt"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/imageView00"
                android:layout_toRightOf="@+id/imageView00"
                android:src="@mipmap/personal_order"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="我的订单"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="35dp"
                android:layout_marginRight="35dp"
                android:text="查看我的订单"
                android:textColor="#c8c8c8"
                android:textSize="13sp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/textView5"
                android:layout_marginEnd="15dp"
                android:layout_marginRight="15dp"
                android:src="@mipmap/personal_enter"/>
        </RelativeLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="68dp"
            android:layout_marginTop="1dp"
            android:background="@color/white">
            <TextView
                android:id="@+id/obligation_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/ll_customer_selector"
                android:clickable="true"
                android:drawableTop="@mipmap/personal_pending_payment"
                android:gravity="center"
                android:paddingTop="10dp"
                android:text="待付款"/>
            <TextView
                android:id="@+id/sendGoods_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/ll_customer_selector"
                android:clickable="true"
                android:drawableTop="@mipmap/personal_waiting_for_delivery"
                android:gravity="center"
                android:paddingTop="10dp"
                android:text="待发货"/>
            <TextView
                android:id="@+id/forGoods_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/ll_customer_selector"
                android:clickable="true"
                android:drawableTop="@mipmap/personal_receiving_goods"
                android:gravity="center"
                android:paddingTop="10dp"
                android:text="待收货"/>
            <TextView
                android:id="@+id/evaluation_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/ll_customer_selector"
                android:clickable="true"
                android:drawableTop="@mipmap/personal_pending_evaluation"
                android:gravity="center"
                android:paddingTop="10dp"
                android:text="待评价"/>
            <TextView
                android:id="@+id/afterSale_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.6"
                android:background="@drawable/ll_customer_selector"
                android:clickable="true"
                android:drawableTop="@mipmap/personal_customer_service"
                android:gravity="center"
                android:paddingTop="10dp"
                android:text="退换/售后"/>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/collect_rlyt"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/imageView4"
                android:layout_toRightOf="@+id/imageView4"
                android:src="@mipmap/personal_collection"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="我的收藏"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true" android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp" android:src="@mipmap/personal_enter"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/discountCoupon_rlyt"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="1dp"
            android:layout_weight="1"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:src="@mipmap/personal_coupon"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="我的优惠券"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true" android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp" android:src="@mipmap/personal_enter"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/personal_address_rl"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="1dp"
            android:layout_weight="1"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/personal_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:src="@mipmap/personal_address"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/personal_address"
                android:layout_toRightOf="@+id/personal_address"
                android:text="收货地址"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true" android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp" android:src="@mipmap/personal_enter"/>
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/feedback_rlyt"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/imageView4"
                android:layout_toRightOf="@+id/imageView4"
                android:src="@mipmap/personal_opinion"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="意见反馈"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp"
                android:src="@mipmap/personal_enter"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/consultation_rlyt"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="1dp"
            android:layout_weight="1"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/imageView4"
                android:layout_toRightOf="@+id/imageView4"
                android:src="@mipmap/personal_service"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="咨询客服"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp"
                android:src="@mipmap/personal_enter"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/share_rlyt"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:background="@drawable/ll_customer_selector"
            android:clickable="true"
        >

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="13dp"
                android:layout_marginStart="13dp"
                android:layout_toEndOf="@+id/imageView4"
                android:layout_toRightOf="@+id/imageView4"
                android:src="@mipmap/personal_share"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="28dp"
                android:layout_marginStart="12dp"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="分享有礼"
                android:textColor="#333333"
                android:textSize="15sp"/>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="14dp"
                android:layout_marginRight="14dp"
                android:src="@mipmap/personal_enter"/>
        </RelativeLayout>

    </LinearLayout>


</LinearLayout>
